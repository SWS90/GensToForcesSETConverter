<?xml version="1.0" encoding="utf-8"?>
<Template>
  <Target type="forcesObjectReference" description="The camera object that this volume activates." />
  <Shape type="sbyte" description="The shape of this volume.">
    <Enum description="Box" value="0" />
    <Enum description="Sphere" value="1" />
    <Enum description="Cylinder" value="2" />
  </Shape>
  <BasePoint type="sbyte" description="Starting point of the volume?">
    <Enum description="At the center" value="0" />
    <Enum description="Z-Axis plane?" value="1" />
