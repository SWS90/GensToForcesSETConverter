  <Width type="float" default="200" description="The width of this volume." />
  <Height type="float" default="1000" description="The height of this volume." />
  <Depth type="float" default="200" description="The depth of this volume." />
  
  <Extra type="description" value="A volume which changes the current camera. Takes precedence over ObjCameraVolumes?" />
  <Extra type="RawByteLength" length="20" />
</Template>